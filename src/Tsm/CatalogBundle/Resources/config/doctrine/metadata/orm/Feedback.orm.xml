<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Feedback" table="feedback">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="companyName" type="string" column="company_name" length="100" nullable="false"/>
    <field name="fullname" type="string" column="fullname" length="512" nullable="false"/>
    <field name="phone" type="string" column="phone" length="100" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <field name="comment" type="string" column="comment" length="1024" nullable="false"/>
  </entity>
</doctrine-mapping>
